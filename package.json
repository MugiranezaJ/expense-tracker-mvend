{
  "name": "expense_tracker_mvend",
  "version": "1.0.0",
  "description": "test app to track down expenses",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "babel src -d build",
    "start": "cross-env NODE_ENV=production npm run migrate && node ./build/app.js",
    "build": "babel src -d dist",
    "devStart": "cross-env NODE_ENV=development nodemon --exec babel-node src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-undo": "npx sequelize-cli db:migrate:undo",
    "migrate-undo-all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-undo": "npx sequelize-cli db:seed:undo"
  },
  "author": "MJackson",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.4.0",
    "@babel/core": "^7.12.3",
    "@babel/register": "^7.11.5",
    "@babel/cli": "^7.12.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5"
    },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
